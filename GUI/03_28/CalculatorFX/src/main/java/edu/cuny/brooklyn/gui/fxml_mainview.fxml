<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane hgap="10.0" vgap="10.0"
	xmlns="http://javafx.com/javafx/8.0.141"
	xmlns:fx="http://javafx.com/fxml/1">
	<rowConstraints>
		<RowConstraints minHeight="10.0" />
		<RowConstraints minHeight="10.0" />
		<RowConstraints minHeight="10.0" />
		<RowConstraints minHeight="10.0" />
	</rowConstraints>
	<columnConstraints>
		<ColumnConstraints minWidth="10.0" prefWidth="100.0" />
		<ColumnConstraints minWidth="10.0" prefWidth="100.0" />
		<ColumnConstraints minWidth="10.0" prefWidth="100.0" />
		<ColumnConstraints minWidth="10.0" prefWidth="100.0" />
		<ColumnConstraints minWidth="10.0" prefWidth="100.0" />
	</columnConstraints>
	<children>
		<Button mnemonicParsing="false" onAction="#processNumberKeys"
			prefHeight="100.0" prefWidth="100.0" text="1" GridPane.rowIndex="1" />
		<Button mnemonicParsing="false" onAction="#processNumberKeys"
			prefHeight="100.0" prefWidth="100.0" text="2"
			GridPane.columnIndex="1" GridPane.rowIndex="1" />
		<Button mnemonicParsing="false" onAction="#processNumberKeys"
			prefHeight="100.0" prefWidth="100.0" text="3"
			GridPane.columnIndex="2" GridPane.rowIndex="1" />
		<Button mnemonicParsing="false" onAction="#processNumberKeys"
			prefHeight="100.0" prefWidth="100.0" text="4" GridPane.rowIndex="2" />
		<Button mnemonicParsing="false" onAction="#processNumberKeys"
			prefHeight="100.0" prefWidth="100.0" text="5"
			GridPane.columnIndex="1" GridPane.rowIndex="2" />
		<Button mnemonicParsing="false" onAction="#processNumberKeys"
			prefHeight="100.0" prefWidth="100.0" text="6"
			GridPane.columnIndex="2" GridPane.rowIndex="2" />
		<Button mnemonicParsing="false" onAction="#processNumberKeys"
			prefHeight="100.0" prefWidth="100.0" text="7" GridPane.rowIndex="3" />
		<Button mnemonicParsing="false" onAction="#processNumberKeys"
			prefHeight="100.0" prefWidth="100.0" text="8"
			GridPane.columnIndex="1" GridPane.rowIndex="3" />
		<Button mnemonicParsing="false" onAction="#processNumberKeys"
			prefHeight="100.0" prefWidth="100.0" text="9"
			GridPane.columnIndex="2" GridPane.rowIndex="3" />
		<Button mnemonicParsing="false" onAction="#processNumberKeys"
			prefHeight="100.0" prefWidth="100.0" text="0"
			GridPane.columnIndex="3" GridPane.rowIndex="3" />
		<Button mnemonicParsing="false" onAction="#processOperatorKeys"
			prefHeight="100.0" prefWidth="100.0" text="-"
			GridPane.columnIndex="3" GridPane.rowIndex="2" />
		<Button mnemonicParsing="false" onAction="#processOperatorKeys"
			prefHeight="100.0" prefWidth="100.0" text="+"
			GridPane.columnIndex="3" GridPane.rowIndex="1" />
		<Button mnemonicParsing="false" prefHeight="320.0"
			prefWidth="100.0" text="=" GridPane.columnIndex="4"
			GridPane.rowIndex="1" GridPane.rowSpan="3" />
		<TextField fx:id="outputTextField" prefHeight="50.0"
			GridPane.columnSpan="5" />
	</children>
	<padding>
		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	</padding>
</GridPane>
